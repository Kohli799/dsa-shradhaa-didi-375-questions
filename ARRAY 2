class Reverse
{
    // Complete the function
    // str: input string
    public static String reverseWord(String str)
    {
        // Reverse the string str
       
        String ss=" ";
        char ch;
        for(int i=0;i<str.length();i++)
        {
        ch=str.charAt(i);
        ss=ch+ss;
        }

        return ss;
    }
    void main()
    {
         String str="Geeks";
        System.out.println(reverseWord(str));
    }
}
